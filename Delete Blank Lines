	

    EXAMPLE/EJEMPLO: FileAppend, % DeleteBlankLines("File.txt"), NewFile.txt
     
    DeleteBlankLines(file){
    if IsFolder(file)
            return 0
     
    IfNotExist, % file, return, 0
           
    Loop,Read,% file
    {
            tgzloop .= A_LoopReadLine "`n"
            if (A_LoopReadLine!="" AND A_LoopReadLine!=A_Space)
                    Count_dbl++
    }
     
    Loop,Parse,tgzloop,`n
    {
            if (A_LoopField!="" AND A_LoopField!=A_Space)
            {
                    index++
                    if (index = Count_dbl)
                    {
                            tgzloop2 .= A_LoopField
                            break
                    }
                    else
                            tgzloop2 .= A_LoopField "`n"
            }
    }
     
    Count_dbl=
    tgzloop=
    file=
    return tgzloop2
    }
    IsFolder(subcomandname:=""){
            FileGetAttrib, Attributes_Check, % subcomandname
            If (!InStr(Attributes_Check,"D"))
                    return 0
            If (InStr(Attributes_Check,"D"))
                    return 1
    }

